  "_moduleAliases": {
    "src": "dist"
  },
  "dependencies": {
    "@guilhermemj/micro-web-server": "^1.1.0",
    "@types/jsonwebtoken": "^8.3.7",
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
